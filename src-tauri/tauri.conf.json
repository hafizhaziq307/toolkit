{
	"build": {
		"beforeDevCommand": "npm run dev",
		"beforeBuildCommand": "npm run build",
		"devPath": "http://localhost:1420",
		"distDir": "../dist",
		"withGlobalTauri": false
	},
	"package": {
		"productName": "toolkit",
		"version": "0.5.0"
	},
	"tauri": {
		"allowlist": {
			"window": {
				"all": true
			},
			"dialog": {
				"all": true
			},
			"shell": {
				"all": false,
				"open": true
			}
		},
		"bundle": {
			"active": true,
			"category": "Utility",
			"copyright": "Â© 2023 hafiz haziq",
			"icon": [
				"icons/32x32.png",
				"icons/128x128.png",
				"icons/128x128@2x.png",
				"icons/icon.icns",
				"icons/icon.ico"
			],
			"identifier": "hafizhaziq.dev.toolkit",
			"targets": "all"
		},
		"security": {
			"csp": null
		},
		"updater": {
			"active": false
		},
		"windows": [
			{
				"fullscreen": false,
				"height": 650,
				"minHeight": 650,
				"resizable": true,
				"decorations": false,
				"title": "toolkit",
				"width": 800,
				"minWidth": 800,
				"center": true
			}
		]
	}
}
